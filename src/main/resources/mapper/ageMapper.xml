<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xue.study.snow.mapper.AgeDAO">

    <insert id="insertAge" parameterType="java.util.Map" >
        insert into ageinfo (ID,age,ageCode,ageName) values (#{Id},#{age},#{ageCode},#{ageName})
    </insert>

</mapper>

